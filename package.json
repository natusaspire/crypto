    "lint:fix": "npm run lint -- --fix",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "keywords": [
    "cryptographic",
    "system",
    "web",
    "crypto",
    "api"
  ],
  "author": "natusaspire",
